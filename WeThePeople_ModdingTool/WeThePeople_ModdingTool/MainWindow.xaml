<Window x:Class="WeThePeople_ModdingTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WeThePeople_ModdingTool"
        mc:Ignorable="d"
        Title="WeThePeople_ModdingTool" Height="450" Width="800" Icon="/WtP_desktop_icon.ico">
    <StackPanel Margin="10" HorizontalAlignment="Left">
        <ComboBox Width="200" x:Name="ComboBox_Yield">
            <ComboBoxItem IsSelected="true">YIELD_FOOD</ComboBoxItem>
            <ComboBoxItem>YIELD_TIMBER</ComboBoxItem>
            <ComboBoxItem>YIELD_HARDWOOD</ComboBoxItem>
        </ComboBox>
        <ComboBox Width="200" x:Name="ComboBox_Harbour">
            <ComboBoxItem IsSelected="true">Europe</ComboBoxItem>
            <ComboBoxItem>Africa</ComboBoxItem>
            <ComboBoxItem>Port Royal</ComboBoxItem>
        </ComboBox>
        <Button Width="160" Height="80">Create</Button>
    </StackPanel>
</Window>
